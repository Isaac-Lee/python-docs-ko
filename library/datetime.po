msgid "\\(1)"
msgstr "\\(1)"

#: /Users/flowdas/works/docs/python-docs-ko/src/Doc/library/datetime.rst:1128
msgid "``datetime2 = datetime1 - timedelta``"
msgstr "``datetime2 = datetime1 - timedelta``"

#: /Users/flowdas/works/docs/python-docs-ko/src/Doc/library/datetime.rst:1128
